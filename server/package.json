    "@types/mock-fs": "^4.13.4",
    "@types/node-cron": "^3.0.11",
    "@types/proper-lockfile": "^4.1.4",
    "@types/redis": "^4.0.10",
    "@types/retry": "^0.12.5",
    "drizzle-kit": "^0.31.1",
    "esbuild": "^0.25.6",
    "esbuild-plugin-tsc": "^0.5.0",
    "mock-fs": "^5.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  }
