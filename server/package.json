{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "build": "node config/esbuild.config.cjs",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts,js,json --exec \"npm run build && node dist/index.js\"",
    "compilets": "npx tsc --incremental",
    "seed": "node dist/scripts/seeder.js",
    "load:tags": "node dist/scripts/cacheLoader.js",
    "db:generate": "drizzle-kit generate --config=./config/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./config/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=./config/drizzle.config.ts",
    "db:push": "drizzle-kit push --config=./config/drizzle.config.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@libsql/client": "^0.15.8",
    "@types/cors": "^2.8.19",
    "better-sqlite3": "^11.10.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "drizzle-seed": "^0.3.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "gel": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.1.0",
    "node-redis": "^0.1.7",
    "nodemon": "^3.1.10",
    "proper-lockfile": "^4.1.2",
    "winston": "^3.17.0"
